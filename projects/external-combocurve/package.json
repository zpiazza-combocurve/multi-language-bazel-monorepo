{
	"name": "external-combocurve",
	"description": "ComboCurve REST API",
	"author": "Inside Petroleum, Inc.",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "babel src --config-file ./babel.config.json --out-dir dist --ignore \"src/**/*.test.(ts|js)\" --extensions \".ts,.js\" --verbose --copy-files --no-copy-ignored",
		"build:dev": "babel . --out-dir out --ignore \"**/node_modules,src/**/*.test.(ts|js),out\" --extensions \".ts,.js\" --verbose --copy-files --no-copy-ignored",
		"dev": "babel-watch ./dev-server.ts --extensions \".ts,.js\" --inspect 0.0.0.0:9228",
		"dev:local": "cross-env DOTENV_CONFIG_PATH=.env.local npm run dev",
		"dev:localdb": "cross-env DOTENV_CONFIG_PATH=.env.localdb npm run dev",
		"gen": "buf generate --path ./proto/combocurve/external ./proto",
		"lint:eslint": "eslint --quiet --color --ext .js,.ts",
		"lint:eslint:ci": "npm run lint:eslint -- --quiet \"**/*.{ts,js}\"",
		"lint:eslint:fix": "npm run lint:eslint -- --fix \"**/*.{ts,js}\"",
		"lint:prettier": "prettier --check",
		"lint:prettier:ci": "npm run lint:prettier --  \"**/*.{ts,js}\"",
		"lint:typescript": "npm run tsc -- --noEmit",
		"lint:typescript:ci": "npm run lint:typescript",
		"lint:ci": "npm run lint:prettier:ci && npm run lint:eslint:ci && npm run lint:typescript:ci",
		"proto": "buf export --path combocurve --output=./proto",
		"proto:stable": "npm run proto -- buf.build/combocurve/dal",
		"repl": "ts-node --transpile-only -r tsconfig-paths/register ./dev-repl",
		"repl:debug": "node -r ts-node/register -r tsconfig-paths/register --inspect ./dev-repl",
		"spec:build": "ts-node --transpile-only -r tsconfig-paths/register spec/build",
		"spec:update": "ts-node --transpile-only -r tsconfig-paths/register spec/update",
		"spec:update:check": "ts-node --transpile-only -r tsconfig-paths/register lib/express-openapi-ts/spec-update-check",
		"start": "node dist/server.js",
		"test": "cross-env NODE_ENV=test jest",
		"test:ci": "npm run test -- --ci --json --coverage --reporters=default --reporters=jest-junit --testLocationInResults --outputFile=report.json",
		"test:watch": "npm run test -- --watch",
		"tsc": "tsc",
		"prepare": "husky install"
	},
	"license": "ISC",
	"dependencies": {
		"@google-cloud/bigquery": "^6.2.0",
		"@google-cloud/logging-winston": "^5.3.0",
		"@google-cloud/opentelemetry-cloud-trace-exporter": "^2.1.0",
		"@google-cloud/opentelemetry-cloud-trace-propagator": "^0.17.0",
		"@opentelemetry/api": "^1.4.1",
		"@opentelemetry/instrumentation-express": "^0.33.0",
		"@opentelemetry/instrumentation-http": "^0.41.1",
		"@opentelemetry/instrumentation-mongodb": "^0.36.0",
		"@opentelemetry/instrumentation-winston": "^0.32.0",
		"@opentelemetry/resources": "^1.15.1",
		"@opentelemetry/sdk-trace-base": "^1.15.1",
		"@opentelemetry/sdk-trace-node": "^1.15.1",
		"@opentelemetry/semantic-conventions": "^1.15.1",
		"@types/async-retry": "^1.4.5",
		"ajv": "^8.12.0",
		"ajv-errors": "^3.0.0",
		"ajv-formats": "^2.1.1",
		"ajv-keywords": "^5.1.0",
		"async-retry": "^1.3.3",
		"body-parser": "^1.19.0",
		"combocurve-utils": "git+ssh://git@combocurve-utils-js.github.com/insidepetroleum/combocurve-utils-js.git#v1.31.2",
		"cors": "^2.8.5",
		"express": "^4.17.3",
		"express-async-errors": "^3.1.1",
		"express-winston": "^4.2.0",
		"fast-safe-stringify": "^2.1.1",
		"handlebars": "^4.7.8",
		"helmet": "^3.23.3",
		"http-status-codes": "^2.2.0",
		"install": "^0.13.0",
		"lodash": "^4.17.21",
		"moment": "^2.27.0",
		"mongoose": "^5.13.20",
		"nice-grpc": "^2.1.4",
		"proj4": "^2.9.0",
		"npm": "^9.7.2",
		"reflect-metadata": "^0.1.13",
		"triple-beam": "^1.3.0",
		"winston": "^3.10.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.22.9",
		"@babel/core": "^7.22.9",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/preset-env": "^7.22.9",
		"@babel/preset-typescript": "^7.22.5",
		"@babel/register": "^7.22.5",
		"@babel/plugin-proposal-decorators": "^7.22.5",
		"@bufbuild/buf": "^1.15.0-1",
		"@types/cors": "^2.8.6",
		"@types/deep-equal": "^1.0.1",
		"@types/express": "^4.17.13",
		"@types/helmet": "0.0.47",
		"@types/jest": "^26.0.4",
		"@types/js-yaml": "^4.0.0",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/lodash": "^4.14.168",
		"@types/mongodb": "^4.0.7",
		"@types/mongoose": "^5.11.97",
		"@types/node": "^14.0.19",
		"@types/proj4": "^2.5.2",
		"@types/triple-beam": "^1.3.2",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"babel-plugin-module-resolver": "^5.0.0",
		"babel-watch": "^7.8.1",
		"cross-env": "^7.0.3",
		"deep-equal": "^2.0.3",
		"deepmerge": "^4.3.1",
		"dotenv": "^16.3.1",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-import-resolver-babel-module": "^5.3.2",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-jest": "^27.2.3",
		"eslint-plugin-prettier": "^5.0.0",
		"husky": "^8.0.3",
		"jest": "^28.0.0",
		"jest-junit": "^14.0.1",
		"js-yaml": "^4.0.0",
		"jsonwebtoken": "^9.0.1",
		"lint-staged": "^10.2.11",
		"mock-express-request": "^0.2.2",
		"mock-express-response": "^0.2.2",
		"mongodb-memory-server-global": "^8.12.0",
		"prettier": "^3.0.0",
		"ts-jest": "^28.0.8",
		"ts-node": "^8.10.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.6"
	},
	"lint-staged": {
		"**/*.{ts,js}": [
			"npm run lint:prettier",
			"npm run lint:eslint"
		]
	},
	"engines": {
		"node": "18.17"
	},
	"volta": {
		"node": "18.17.0",
		"npm": "9.6.7"
	}
}
